#!/bin/sh

# This asdf shell script installs an already downloaded version of
# verifast

# NOTE: at the moment this ONLY works on Linux using an Intel processor.

echo "------------------------------------------------------------------"
echo "ASDF_DOWNLOAD_PATH: $ASDF_DOWNLOAD_PATH"
echo " ASDF_INSTALL_PATH: $ASDF_INSTALL_PATH"
echo "------------------------------------------------------------------"

mv $ASDF_DOWNLOAD_PATH/* $ASDF_INSTALL_PATH
